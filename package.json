    }
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4.4.4"
  },
  "scripts": {
    "start": "node standalone.js",
    "test": "standard && tap --reporter=spec test/*.js",
    "coverage": "tap test/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/*.js && cat coverage/lcov.info | coveralls",
    "setup": "npm install && node setup.js",
    "generate-test-token": "node utils/generate-test-token.js"
  },
  "keywords": [
    "Louie",
    "Telemark fylkeskommune",
    "Tfk",
    "Digitalisering",
    "Skoleskyss"
  ],
  "standard": {
    "ignore": [
      "public"
    ]
  },
  "louie": {
    "versionName": "Mötorhead",
    "versionVideoUrl": "https://www.youtube.com/watch?v=4V1p1dM3snQ",
    "systemName": "skoleskyss"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/telemark/skoleskyss.t-fk.no.git"
  },
  "bugs": {
    "url": "http://github.com/telemark/skoleskyss.t-fk.no/issues"
  },
  "devDependencies": {
    "coveralls": "2.11.6",
    "standard": "7.0.1",
    "tap": "5.4.4"
  },
  "dependencies": {
    "get-skole-aar": "1.0.0",
    "good": "7.0.0",
    "good-console": "6.1.0",
    "good-squeeze": "3.0.1",
    "handlebars": "4.0.5",
    "hapi": "13.0.0",
    "hapi-auth-cookie": "6.0.0",
    "hapi-auth-jwt2": "6.0.0",
    "hoek": "4.0.0",
    "inert": "4.0.0",
    "jsonwebtoken": "6.0.0",
    "mongojs": "2.3.0",
    "vision": "4.0.1",
    "wreck": "7.0.0"
  }
}
