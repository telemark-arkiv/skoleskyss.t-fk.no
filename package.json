    }
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4.6.0"
  },
  "scripts": {
    "start": "node standalone.js",
    "test": "standard &&  nsp check && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "setup": "npm install && node setup.js",
    "standard-fix": "standard --fix",
    "generate-test-token": "node utils/generate-test-token.js"
  },
  "keywords": [
    "Louie",
    "Telemark fylkeskommune",
    "Tfk",
    "Digitalisering",
    "Skoleskyss"
  ],
  "standard": {
    "ignore": [
      "public"
    ]
  },
  "louie": {
    "versionName": "Motörhead",
    "versionVideoUrl": "https://www.youtube.com/watch?v=WDP7_h4wkgw",
    "systemName": "skoleskyss"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/telemark/skoleskyss.t-fk.no.git"
  },
  "bugs": {
    "url": "http://github.com/telemark/skoleskyss.t-fk.no/issues"
  },
  "devDependencies": {
    "coveralls": "2.11.15",
    "nsp": "2.6.2",
    "standard": "8.6.0",
    "tap": "8.0.1"
  },
  "dependencies": {
    "chairo": "3.0.0",
    "get-skole-aar": "1.0.0",
    "good": "7.1.0",
    "good-console": "6.3.1",
    "good-squeeze": "5.0.1",
    "handlebars": "4.0.6",
    "hapi": "16.0.0",
    "hapi-auth-cookie": "git+https://github.com/arnivuo/hapi-auth-cookie",
    "hapi-auth-jwt2": "7.2.3",
    "hoek": "4.1.0",
    "inert": "4.0.1",
    "jsonwebtoken": "7.1.9",
    "mongojs": "2.4.0",
    "seneca": "3.2.2",
    "seneca-balance-client": "0.6.0",
    "seneca-mesh": "0.9.0",
    "simple-encryptor": "1.1.0",
    "tfk-saksbehandling-skoleskyss-waypoints": "1.0.4",
    "vision": "4.1.0",
    "wreck": "10.0.0",
    "yar": "8.1.1"
